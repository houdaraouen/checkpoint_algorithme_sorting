ALGORITHM sorting
//declaration de variable
VAR
    T array_name : ARRAY_OF INTEGER [n];
    i,j,tmp:INTEGER;
BEGIN
//initialisation du tableau
FOR i FROM i=lenght(T)-1 TO 1 STEP -1  DO
       FOR j FROM 1 TO i STEP 1  DO
        IF (T[j]>T[j+1]) THEN
            tmp:=T[j];
            T[j]:=T[j+1];
            T[j+1]:=tmp;

        END_IF
       END_FOR
    
END_FOR
    
END